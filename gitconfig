[color]
    status = auto
    branch = auto
    diff = auto
    ui = true
    pager = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    who = shortlog -s -n --no-merges
    standup = shortlog --since=yesterday
    today = shortlog --since=midnight
    uncommit = reset --soft HEAD^
    unref = reset --soft HEAD@{1}
    subs = submodule foreach git pull origin master
    hist = recent-branches
[push]
    default = matching
